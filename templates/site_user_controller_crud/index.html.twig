{% extends 'base.html.twig' %}

{% block title %}SiteUser index{% endblock %}

{% block body %}
    <div class="container">
        <h1>SiteUser index</h1>

        <table class="table">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>actions</th>
                </tr>
            </thead>
            <tbody>
            {% for site_user in site_users %}
                <tr>
                    <td>{{ site_user.id }}</td>
                    <td>
                        <a href="{{ path('app_site_user_controller_crud_show', {'id': site_user.id}) }}">show</a>
                        <a href="{{ path('app_site_user_controller_crud_edit', {'id': site_user.id}) }}">edit</a>
                    </td>
                </tr>
            {% else %}
                <tr>
                    <td colspan="2">no records found</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>

        <a href="{{ path('app_site_user_controller_crud_new') }}">Create new</a>
        </div>
{% endblock %}
