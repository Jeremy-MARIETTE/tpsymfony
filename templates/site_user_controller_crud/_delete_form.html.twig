<form method="post" action="{{ path('app_site_user_controller_crud_delete', {'id': site_user.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ site_user.id) }}">
    <button class="btn">Delete</button>
</form>
