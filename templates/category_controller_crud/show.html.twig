{% extends 'base.html.twig' %}

{% block title %}Category{% endblock %}

{% block body %}
<div class="container">
    <h1>Category</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ category.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ category.nom }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_category_controller_crud_index') }}">back to list</a>

    <a href="{{ path('app_category_controller_crud_edit', {'id': category.id}) }}">edit</a>

    {{ include('category_controller_crud/_delete_form.html.twig') }}
</div>
{% endblock %}
