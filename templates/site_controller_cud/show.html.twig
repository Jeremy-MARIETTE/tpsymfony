        <h1>Site</h1>

        <table class="table">
            <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ site.id }}</td>
                </tr>
                <tr>
                    <th>Nom</th>
                    <td>{{ site.nom }}</td>
                </tr>
                <tr>
                    <th>Cp</th>
